{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "blackfireio/blackfire-symfony-meta": "^1.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "lexik/jwt-authentication-bundle": "^2.20",
        "nelmio/cors-bundle": "^2.4",
        "phauthentic/correlation-id-symfony-bundle": "dev-develop",
        "phauthentic/error-response": "dev-master",
        "phauthentic/event-sourcing": "dev-master",
        "phauthentic/event-store": "dev-master",
        "phauthentic/snapshot-store": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.27",
        "symfony/amqp-messenger": "7.0.*",
        "symfony/asset": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/doctrine-messenger": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.0.*",
        "symfony/mailer": "7.0.*",
        "symfony/messenger": "7.0.*",
        "symfony/property-access": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/translation": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/uid": "7.0.*",
        "symfony/validator": "7.0.*",
        "symfony/yaml": "7.0.*",
        "symfonycorp/platformsh-meta": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "recreate-db": [
            "bin/console doctrine:database:drop --force",
            "bin/console doctrine:database:create",
            "bin/console doctrine:migrations:migrate --no-interaction"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*",
            "docker": true
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Phauthentic/snapshot-store.git"
        },
        {
            "type": "git",
            "url": "git@github.com:Phauthentic/event-sourcing.git"
        },
        {
            "type": "git",
            "url": "git@github.com:Phauthentic/event-store.git"
        },
        {
            "type": "git",
            "url": "git@github.com:Phauthentic/error-response.git"
        },
        {
            "type": "git",
            "url": "git@github.com:Phauthentic/correlation-id-symfony-bundle.git"
        }
    ],
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "7.0.*",
        "symfony/maker-bundle": "^1.57",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*"
    }
}
